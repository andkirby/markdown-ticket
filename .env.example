# Frontend Logging Auto-Start Configuration
# Set to true to automatically start frontend logging sessions
# This provides continuous console log capture for development
# Can be overridden at runtime with URL parameter: ?frontend_logging_autostart=true
VITE_FRONTEND_LOGGING_AUTOSTART=false

# EventBus Logging Configuration
# Set to true to disable EventBus debug logging in development
# This reduces console noise from event subscriptions/emissions
# Only applies to development mode (import.meta.env.DEV)
VITE_DISABLE_EVENTBUS_LOGS=false